{
  "id": "/kafka-manager",
  "cmd": null,
  "cpus": 1,
  "mem": 2048,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": true,
      "image": "deric/kafka-manager:1.3.3.16",
      "parameters": [],
      "privileged": false
    },
    "volumes": [],
    "portMappings": [
      {
        "containerPort": 9000,
        "hostPort": 9000,
        "labels": {
          "VIP_0": "/kafka-manager:9000"
        },
        "protocol": "tcp",
        "servicePort": 9000
      }
    ]
  },
  "env": {
    "ZK_HOSTS": "zk-1.zk:2181,zk-2.zk:2181,zk-3.zk:2181,zk-4.zk:2181,zk-5.zk:2181",
    "KAFKA_MANAGER_LOGLEVEL": "INFO",
    "HTTP_CONTEXT": "/",
    "APPLICATION_SECRET": "secret",
    "KAFKA_MANAGER_ARGS": "",
    "BASE_ZK_PATH": "/kafka-manager",
    "KAFKA_MANAGER_CONFIG": "./conf/application.conf"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 120,
      "intervalSeconds": 30,
      "maxConsecutiveFailures": 0,
      "path": "/api/health",
      "portIndex": 0,
      "protocol": "MESOS_HTTP",
      "ipProtocol": "IPv4",
      "timeoutSeconds": 5,
      "delaySeconds": 15
    }
  ],
  "labels": {
    "DCOS_PACKAGE_OPTIONS": "",
    "DCOS_SERVICE_SCHEME": "http",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_SERVICE_NAME": "kafka-manager",
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_VERSION": "1.1.0-1.3.3.16",
    "DCOS_PACKAGE_NAME": "kafka-manager"
  },
  "networks": [
    {
      "name": "dcos",
      "mode": "container"
    }
  ]
}
